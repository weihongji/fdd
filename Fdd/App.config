<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- List of fdd files which are separated by comma -->
		<add key="fdd_files" value="fdd1_white.txt, fdd2_black.txt, fdd3_thomas.txt, fdd4_dbintanet02.txt" />
		
		<!-- Part of the name to show as Backup.Location -->
		<!-- Assuming the file name is fdd1_white.txt, see examples of (regexp) patterns and location got from the pattern below. -->
		<!-- Pattern: * ==> Location: fdd1_white -->
		<!-- Pattern: _(.*)\.txt ==> Location: white -->
		<add key="location_pattern" value="_(.*)" />

		<!-- List item format. Available values: raw, parsed -->
		<add key="item_format" value="parsed" />
		
		<!-- Format when an entry is shown as search result -->
		<!-- Assuming backup file is ajax_db_20200303230204.BAK, see examples of format and result below: -->
		<!-- Format: (db) of (date) ((location)) - (size) ==> ajax of 2020-03-03 (dbintanet02) - 3,536,537 KB -->
		<!-- Format: (location): (db) of (date), (size) ==> dbintanet02: ajax of 2020-03-03, 3,536,537 KB -->
		<add key="format_pattern" value="" />
		
		<!-- List item format. Show size or not -->
		<add key="show_size" value="true" />

		<!-- Status bar: Show timestamp of last search or not -->
		<add key="show_last_search_time" value="true" />
		
		<!-- Status bar: Show details of searching -->
		<add key="show_search_detail" value="true" />
	</appSettings>
</configuration>